<!DOCTYPE html>
<html>
  <head>
    <title>Inventory App | Categories</title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <header>
      <div>Home</div>
      <div>Categories</div>
      <div>Items</div>
      <div>Add Category</div>
      <div>Add Item</div>
    </header>

    <main>
      <div class="ctitle">Categories</div>

      <% allCategory.forEach((i) => { %>
      <div class="categories">
        <div>- <%= i.name %></div>
        <div><%= i.desc%></div>
      </div>
      <hr />
      <% }) %>
    </main>
  </body>
</html>
